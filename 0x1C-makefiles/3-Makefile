CC = gcc
SRC = main.c school.c
OBJ = $(SRC:.c=.o)
NAME = school
RM = rm -f

# make all == make executable
all: m.h $(OBJ)
	$(CC) $(OBJ) -o $(NAME)

# deletes tmp files and executable
clean:
	$(RM) *~ $(NAME)

# delete object files
oclean:
	$(RM) $(OBJ)

#deletes all (tmp files, executable, and obj files)
fclean: clean clean

#deletes all and recompile all source file
re: oclean all

